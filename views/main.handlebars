<button type="button" name="new-post">New Post</button>
<div class="posts-container">
    <h2>Posts</h2>
    {{#each posts as |post|}}
    <div class="individual-post">
        <h3>
            {{post.post_title}}
        </h3>
        <p>
            {{post.post_text}}
        </p>
    </div>
    {{/each}}
</div>